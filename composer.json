{
    "name": "ekojs/sppt-client",
    "description": "Worker SPPT - TI",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eko Junaidi Salam",
            "email": "eko.junaidi.salam@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "symfony/console": "^5.1",
        "adodb/adodb-php": "^5.20"
    },
    "autoload": {
        "psr-4": {
            "EkoJunaidiSalam\\App\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit --testdox",
        "coverage": "vendor/bin/phpunit --colors --testdox --coverage-html build/coverage --coverage-text",
        "reload": [
            "@composer dump-autoload"
        ]
},
    "autoload-dev": {
        "psr-4": {
            "EkoJunaidiSalam\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    }
}
